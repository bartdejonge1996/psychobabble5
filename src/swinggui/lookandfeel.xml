<synth>


  <style id="basicStyle">
	    <font name="Avenir" size="12"/>
	    <state>
		      <color value="WHITE" type="BACKGROUND"/>
		      <color value="BLACK" type="FOREGROUND"/>
	    </state>
  </style>
 
 
  <style id="basicbuttonStyle">
	  	<insets top="20" left="50" bottom="25" right="50"/>
		<state>
		   		<font name="Avenir" size="18"/>
		   		<color value="#918f8f" type="TEXT_FOREGROUND"/>
				<imagePainter method="buttonBackground" path="images/button_stock.png" sourceInsets="30 35 35 35"/>
				<color value="#FFFFFF" type="FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER">
				<font name="Avenir" size="18"/>
				<color value="#747272" type="TEXT_FOREGROUND"/>
				<imagePainter method="buttonBackground" path="images/button_mouseover.png" sourceInsets="30 35 35 35"/>
		</state>
		<state value="MOUSE_OVER and PRESSED">
				<font name="Avenir" size="18"/>
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
				<imagePainter method="buttonBackground" path="images/button_mouseover.png" sourceInsets="30 35 35 35"/>
		</state>
		<state value="DISABLED">
				<font name="Avenir" size="18"/>
				<color value="#bdbbbb" type="TEXT_FOREGROUND"/>
				<imagePainter method="buttonBackground" path="images/button_disabled.png" sourceInsets="30 35 35 35"/>
		</state>
  </style>
  
  <style id="basictextfieldStyle">
	  	<insets top="5" left="10" bottom="5" right="10"/>
		<state>
		   		<font name="Avenir" size="14"/>
		   		<color value="#918f8f" type="TEXT_FOREGROUND"/>
				<imagePainter method="textfieldBackground" path="images/textfield.png" sourceInsets="11 11 11 11"/>
				<color value="#FFFFFF" type="FOREGROUND"/>
		</state>
		<state value="DISABLED">
		   		<font name="Avenir" size="14"/>
		   		<color value="#bdbbbb" type="TEXT_FOREGROUND"/>
				<imagePainter method="textfieldBackground" path="images/textfield_disabled.png" sourceInsets="11 11 11 11"/>
				<color value="#FFFFFF" type="FOREGROUND"/>
		</state>
  </style>
  
    <style id="basicformatStyle">
	  	<insets top="5" left="10" bottom="5" right="10"/>
		<state>
		   		<font name="Avenir" size="14"/>
		   		<color value="#918f8f" type="TEXT_FOREGROUND"/>
				<imagePainter method="formattedtextfieldBackground" path="images/textfield.png" sourceInsets="11 11 11 11"/>
				<color value="#FFFFFF" type="FOREGROUND"/>
		</state>
		<state value="DISABLED">
		   		<font name="Avenir" size="14"/>
		   		<color value="#bdbbbb" type="TEXT_FOREGROUND"/>
				<imagePainter method="formattedtextfieldBackground" path="images/textfield_disabled.png" sourceInsets="11 11 11 11"/>
				<color value="#FFFFFF" type="FOREGROUND"/>
		</state>
  </style>
  
  <style id="headerouter">
	  	<insets top="15" left="70" bottom="50" right="55"/>
	  	<font name="Avenir" size="20" style="Roman"/>
	  	<imagePainter method="buttonBackground" path="images/Header/Header_Outer.png" sourceInsets="0 0 0 0"/>
	  	<state>
	   			<color value="#918f8f" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER">
				<color value="#747272" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and PRESSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
  </style>
  
  
  <style id="headerouter2">
	  	<insets top="15" left="55" bottom="50" right="70"/>
	  	<font name="Avenir" size="20" style="Roman"/>
	  	<imagePainter method="buttonBackground" path="images/Header/Header_Outer2.png" sourceInsets="0 0 0 0"/>
	  	<state>
	   			<color value="#918f8f" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER">
				<color value="#747272" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and PRESSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
  </style>
  
  
  <style id="headermiddle">
	  	<insets top="15" left="55" bottom="50" right="55"/>
	  	<font name="Avenir" size="20" style="Roman"/>
	  	<imagePainter method="buttonBackground" path="images/Header/Header_Middle.png" sourceInsets="0 0 0 0"/>
	  	<state>
	   			<color value="#918f8f" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER">
				<color value="#747272" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and PRESSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
		<state value="MOUSE_OVER and FOCUSED">
				<color value="#73d0ff" type="TEXT_FOREGROUND"/>
		</state>
  </style>
  
  <style id="headerinner">
	  	<insets top="46" left="40" bottom="47" right="40"/>
	  	<font name="Avenir" size="20" style="Roman"/>
	  	<state>
	   		<imagePainter method="buttonBackground" path="images/Header/Header_Inner.png" sourceInsets="0 0 0 0"/>
		</state>
		<state value="MOUSE_OVER">
			<imagePainter method="buttonBackground" path="images/Header/Header_Inner_MOUSE.png" sourceInsets="0 0 0 0"/>
		</state>
  </style>
  
  
  <style id = "basicpanelStyle">
	  	<insets top="32" left="47" bottom="50" right="47"/>
	  	<opaque value="FALSE"/>
	  	<imagePainter method="panelBackground" path="images/panel.png" sourceInsets="60 60 50 60"/>
  </style>
  
    <style id = "bottompanelStyle">
    	<insets top="13" left="47" bottom="0" right="47"/>
	  	<opaque value="FALSE"/>
	  	<imagePainter method="panelBackground" path="images/southpanel.png" sourceInsets="60 60 0 60"/>
  </style>
  
  
  <style id = "basiclabelStyle">
		<state>
		  		<font name="Avenir" size="26" style="Book"/>
		  		<color value="#918f8f" type="TEXT_FOREGROUND"/>
	  	</state>
  </style>
  
  
  <style id = "shirtlabelStyle">
	  <state>
	  		<insets top="35" left = "35" bottom = "35" right = "35"/>
	  		<opaque value="FALSE"/>
	  		<color value="#FFFFFF" type="TEXT_FOREGROUND"/>
	  		<imagePainter method="labelBackground" path="images/T-Shirt.png" sourceInsets ="0 0 0 0"/>
	  </state>
  </style>
  
  
  <style id = "basicLineSeparatorStyle">
  		<imagePainter method="labelBackground" path="images/Line_Separator.png" sourceInsets="0 0 0 0"/>
  </style>
  
  <style id="ScrollBar">
  		<property key="ScrollBar.minimumThumbSize" type="dimension" value="0 40"/>
  		<property key="ScrollBar.squareButtons" type="boolean" value="true"/>
  		<state>
    			<color value="BLACK" type="BACKGROUND"/>
      			<imagePainter method="scrollBarBackground" direction="vertical" path="images/scrollbutton.png" sourceInsets="14 0 14 0" />
    	</state>
  </style>
  
  <style id="ScrollBarThumbStyle">
    	<opaque value="FALSE"/>
    	<insets top="0" left="0" bottom="0" right="0"/>
    	<state>
    			<color value="BLACK" type="BACKGROUND"/>
      			<imagePainter method="scrollBarThumbBackground" direction="vertical" path="images/scrollbar.png" sourceInsets="15 0 15 0" />
    	</state>
    	<state value="MOUSE_OVER">
				<color value="BLACK" type="BACKGROUND"/>
      			<imagePainter method="scrollBarThumbBackground" direction="vertical" path="images/scrollbar_over.png" sourceInsets="15 0 15 0" />
		</state>
  </style>
  
  <style id = "playerpanelStyle">
	  	<insets top="0" left="4" bottom="2" right="0"/>
	  	<opaque value="FALSE"/>
	  	<imagePainter method="panelBackground" path="images/playerpanel.png" sourceInsets="10 10 30 10"/>
  </style> 

  
  <bind style="basicStyle" type="region" key=".*"/>
  <bind style="basiclabelStyle" type="region" key="Label"/>
  <bind style="basicpanelStyle" type="name" key="Panel"/>
  <bind style="bottompanelStyle" type="name" key="Southpanel"/>
  <bind style="playerpanelStyle" type="name" key="playerPanel"/>
  <bind style="basicbuttonStyle" type="name" key="Test"/>
  <bind style="basictextfieldStyle" type="region" key="TextField"/>
  <bind style="shirtlabelStyle" type="name" key="shirtLabel"/>
  
  <bind style="ScrollBarThumbStyle" type="region" key="ScrollBarThumb"/>
  <bind style="ScrollBar" type="region" key="ScrollBar"/>
  
  <bind style="headerouter" type="name" key="HeaderOuter"/>
  <bind style="headerouter2" type="name" key="HeaderOuter2"/>
  <bind style="headermiddle" type="name" key="HeaderMiddle"/>
  <bind style="headerinner" type="name" key="HeaderInner"/>
   
   
</synth>